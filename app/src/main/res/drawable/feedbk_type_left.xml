<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item >
        <shape>
            <corners android:topLeftRadius="10dp" android:topRightRadius="0.1dp"
                android:bottomRightRadius="0.1dp" android:bottomLeftRadius="10dp" />
            <stroke android:width="1dp" android:color="@color/btn_green" />
        </shape>
    </item>
    <item android:top="1dp" android:left="1dp" android:bottom="1dp">
        <shape>
            <solid android:color="@color/white" />
            <corners android:topLeftRadius="10dp" android:topRightRadius="0.1dp"
                android:bottomRightRadius="0.1dp" android:bottomLeftRadius="10dp" />
        </shape>
    </item>
</layer-list>